{
  "extends": [
    "@react-native-community",
    "prettier",
    "prettier/react",
    "prettier/@typescript-eslint"
  ],
  "rules": {
    "quotes": ["error", "double", { "avoidEscape": false }],
    "semi": 1
  },
  "parser": "@typescript-eslint/parser",
  "plugins": [
    "react",
    "react-native",
    "react-hooks",
    "@typescript-eslint",
    "prettier"
  ]
}
